{
  "name": "@djpoida/ha-bom-weather-card",
  "version": "0.0.1154",
  "author": "Peter Eldred",
  "license": "MIT",
  "description": "A Home Assistant card designed to display weather information in the style of the BOM (Bureau of Meteorology) Australia app",
  "main": "dist/ha-bom-weather-card.js",
  "module": "dist/ha-bom-weather-card.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "serve": "rollup -c --watch",
    "hass:start": "docker-compose up -d",
    "hass:restart": "docker-compose restart",
    "hass:stop": "docker-compose down",
    "hass:clean": "node tools/clean-hass-dev.mjs",
    "install:bureau_of_meteorology": "docker-compose exec homeassistant /config/scripts/install_bureau_of_meteorology.sh && docker restart homeassistant",
    "install:hacs": "docker-compose exec homeassistant /config/scripts/install_hacs.sh && docker restart homeassistant"
  },
  "dependencies": {
    "custom-card-helpers": "^1.9.0",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "classnames": "^2.5.1",
    "eslint": "^8.20.0",
    "eslint-plugin-lit": "^1.15.0",
    "loglevel": "^1.9.2",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup": "^4.9.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-svg": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  }
}